@page "/actors"
@using ProyPelis.Client.Pages.Components
@inject IServiceActor actor

<ActorCardComponent Actors = "Actors"></ActorCardComponent>

@code{
    @* Creamos instancia de la interfaz *@
    private List<Actor>Actors;
    protected override void OnInitialized(){
        Actors = actor.GetActors();
    }
}